        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WxBot"
        mc:Ignorable="d"
        Title="登陆" Height="300" Width="300" ResizeMode="NoResize">
    <Grid>
        <Image Name="QRCode" HorizontalAlignment="Left" Height="200" Margin="50,40,0,0" VerticalAlignment="Top" Width="200"/>
        <RichTextBox Name="code" HorizontalAlignment="Left" Height="200" Margin="50,40,0,0" VerticalAlignment="Top" Width="200">
            <FlowDocument/>
        </RichTextBox>

    </Grid>
</Window>
